      </program>
      <program name="apertium-postchunk">
        <file name="apertium-sme-nob.sme-nob.t4x"/>
        <file name="sme-nob.t4x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sme-nob.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

