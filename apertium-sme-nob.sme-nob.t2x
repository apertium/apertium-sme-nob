            <lit v="caseprep"/><lit-tag v="PR"/><lit v="{^med"/><lit-tag v="pr"/><lit v="$}"/>
          </chunk>
          <b pos="2"/>
          <chunk><clip pos="1" part="whole"/></chunk>
