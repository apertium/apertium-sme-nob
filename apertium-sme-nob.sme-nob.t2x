        <pattern-item n="SN"/>
        <pattern-item n="vcop-FV"/>
      </pattern>
      <action>
        <call-macro n="switch_cases"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <call-macro n="set_ana_gen"><with-param pos="2"/></call-macro>
        <call-macro n="set_ana_gen"><with-param pos="3"/></call-macro>
