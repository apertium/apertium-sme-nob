np: _.npclass.gender;
adj_plain: _.sint.comp.gender.number.defnes;
adj: (always
       1(adj_plain)[gender=(if (1.defnes = def or 1.number = pl) un
              elif (1.gender = f or 1.gender = m) mf
              else 1.gender), 
              number=(if (1.defnes = def) sp else 1.number)]);
