        <call-macro n="modify_nomcmp_nbr_art"><with-param pos="1"/></call-macro>

        <call-macro n="copy_syn_label"><with-param pos="2"/><with-param pos="1"/></call-macro>
        <call-macro n="set_defnes"><with-param pos="2"/></call-macro>
        <let>
          <var n="pre-nom"/>
