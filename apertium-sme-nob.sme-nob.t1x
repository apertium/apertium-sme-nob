          <let><clip pos="2" side="tl" part="nbr"/><lit-tag v="sg"/></let>
        </when></choose>

        <choose><when c="Remove dash from spelled out numbers preceding non-numbers (tiåret, not ti-året)">
          <test><or>
            <equal><clip pos="1" side="sl" part="a_cmp"/><lit-tag v="cmp_sgnom.cmp_splitr"/></equal>
            <equal><clip pos="1" side="sl" part="a_cmp"/><lit-tag v="cmp_sggen.cmp_splitr"/></equal>
          </or></test>
          <let><clip pos="1" side="tl" part="a_cmp"/><lit v=""/></let>
