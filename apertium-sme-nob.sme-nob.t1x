                  <tag><lit-tag v="ind"/></tag>
                  <tag><clip pos="2" side="sl" part="mode"/></tag>
                  <tag><var n="c_gen"/></tag>
                  <tag><var n="c_nbr"/></tag>
                </tags>
                <var n="pre-nom"/>
                <lu>
                  <lit v="årig"/>
                  <lit-tag v="adj.pst.nt.sg.ind"/>
                </lu>
              </chunk>
            </out>
            <choose>
              <when>
                <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
                <out><b pos="1"/></out>
              </when>
            </choose>
          </when>

          <otherwise>
            <call-macro n="out_nom"><with-param pos="2"/></call-macro>
          </otherwise>
        </choose>
      </action>
    </rule>

