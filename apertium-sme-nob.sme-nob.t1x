          <otherwise>
            <let><var n="tmp"/><lit-tag v="3"/></let>
          </otherwise>
        </choose>
        <choose>
          <when>
            <test><begins-with><clip pos="2" side="tl" part="lemh"/><lit v="-"/></begins-with></test>
