            </out>
          </when>
          <when c="selv.det.emph">
            <test><and>
              <not><equal><clip pos="1" side="tl" part="a_det"/><lit v=""/></equal></not>
              <not><equal><clip pos="1" side="tl" part="syn_label"/><lit-tag v="@Pron←"/></equal></not>
