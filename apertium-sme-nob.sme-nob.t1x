          <let><var n="vfin-pers"/><clip pos="1" side="sl" part="pers"/></let>
          <let><var n="vfin-nbr"/><clip pos="1" side="sl" part="nbr"/></let>
          <let><var n="vfin-temps"/><clip pos="1" side="sl" part="fintemps"/></let>
